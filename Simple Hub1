--UGC
UGC.newLabel("Get items instantly from OLD Events")

UGC.newButton("Island Of Move", "Script is Working", function()
    local Codes = {"WorldAlive","DIY","SettingTheStage","StrikeAPose","GetMoving","VictoryLap","ThingsGoBoom","FXArtist","Boardwalk","ParticleWizard","Glimmer"}
for i = 1,#Codes do
    game:GetService("ReplicatedStorage").Events.RedeemCode:FireServer(Codes[i])
end
end)

UGC.newButton("Galactic Speedway Creator Challenge", "Script is Working", function()
    local Codes = {"Lesson1","Lesson2","Lesson3","Lesson4","Lesson5","Lesson6"}
for i = 1,#Codes do
    game:GetService("ReplicatedStorage").Events.FinishQuiz:InvokeServer(Codes[i])
end
end)

