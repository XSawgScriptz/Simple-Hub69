--Fun Stuff
Fun.newLabel("Below, all scripts are Operational and Safe")

-- Function to get the player's humanoid
local function getHumanoid(player)
    local character = player.Character or player.CharacterAdded:Wait()
    return character:WaitForChild("Humanoid")
end

-- Get the player
local player = game.Players.LocalPlayer
local humanoid = getHumanoid(player)

-- Minimum and maximum speed values
local minSpeed = 16
local maxSpeed = 100

--Speed Slider
Fun.newSlider("Speed Slider", "Adjust your speed", 100, false, function(num)
    -- Normalize the slider value to a 0-1 range
    local normalizedValue = num / 100
    
    local newSpeed = minSpeed + (normalizedValue * (maxSpeed - minSpeed))
    
    --Update Speed
    humanoid.WalkSpeed = newSpeed
    
    -- Print the new speed (for debugging)
    print("Humanoid's new speed:", newSpeed)
end)

--jump power slider
-- Function to get the player's humanoid
local function getHumanoid(player)
    local character = player.Character or player.CharacterAdded:Wait()
    return character:WaitForChild("Humanoid")
end

-- Get the player
local player = game.Players.LocalPlayer
local humanoid = getHumanoid(player)

-- Minimum and maximum jump power values
local minJumpPower = 50
local maxJumpPower = 200

-- Create the slider with the callback function
Fun.newSlider("Jump Power Slider", "Adjust your jump power", 100, false, function(num)
    -- Normalize the slider value to a 0-1 range
    local normalizedValue = num / 100
    
    -- Calculate the new jump power
    local newJumpPower = minJumpPower + (normalizedValue * (maxJumpPower - minJumpPower))
    
    -- Update the humanoid's JumpPower
    humanoid.JumpPower = newJumpPower
    
    -- Print the new jump power (for debugging)
    print("Humanoid's new jump power:", newJumpPower)
end)

--Gravity Slider
-- Reference to the Workspace
local workspace = game.Workspace

-- Minimum and maximum gravity values (Roblox default is 196.2)
local minGravity = 50
local maxGravity = 500

-- Create the slider with the callback function
Fun.newSlider("Gravity Slider", "Adjust the gravity", 100, false, function(num)
    -- Normalize the slider value to a 0-1 range
    local normalizedValue = num / 100
    
    -- Calculate the new gravity
    local newGravity = minGravity + (normalizedValue * (maxGravity - minGravity))
    
    -- Update the workspace's gravity
    workspace.Gravity = newGravity
    
    -- Print the new gravity (for debugging)
    print("New gravity:", newGravity)
end)

-- FOV

-- Get the player and the camera
local player = game.Players.LocalPlayer
local camera = game.Workspace.CurrentCamera

-- Define the normal and increased FOV values
local normalFOV = 70
local increasedFOV = 100

-- Create the toggle with the callback function
Fun.newToggle("FOV Toggle", "Increase FOV", true, function(toggleState)
    if toggleState then
        -- Set FOV to increased value
        camera.FieldOfView = increasedFOV
        print("FOV: Increased")
    else
        -- Set FOV to normal value
        camera.FieldOfView = normalFOV
        print("FOV: Normal")
    end
end)

